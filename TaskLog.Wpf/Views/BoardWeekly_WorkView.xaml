<views:MvxWpfView 
    x:Class="TaskLog.Wpf.Views.BoardWeekly_WorkView"
    x:TypeArguments="viewModels:WorkViewModel"
    xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
    xmlns:mvx="clr-namespace:MvvmCross.Platforms.Wpf.Binding;assembly=MvvmCross.Platforms.Wpf"
    xmlns:local="clr-namespace:TaskLog.Wpf.Views"
    xmlns:viewModels="clr-namespace:TaskLog.Core.ViewModels;assembly=TaskLog.Core"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    mc:Ignorable="d" Background="AntiqueWhite"
    d:DesignHeight="450" d:DesignWidth="800">

    <Grid>
        <TextBlock Text="{Binding Header, FallbackValue='Work Header'}"></TextBlock>
    </Grid>
</views:MvxWpfView>
